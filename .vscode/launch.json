{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: Dev Server",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "port": 9229,
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "development"
      },
      "outFiles": ["${workspaceFolder}/.next/**/*.js"]
    }
  ]
}
